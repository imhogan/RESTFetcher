<?xml version="1.0" encoding="UTF-8"?>
<qrvey name='Resident Interest Survey'>
    <settings>
        <properties introPage='true' emailNotifications='true' results_visibility="Private" respondent_email="Mandatory"></properties>
        <description>Please fill out this survey to help the Social Committee plan events to suit your interests. Please note, your responses are anonymous.</description>
        <duration>
            <neverExpires>true</neverExpires>
            <from>2017-10-03T00:00:00.000Z</from>
        </duration>
        <thankYouPage>
            <title>Thanks for registering your interests.</title>
            <content>The Social Committee appreciates the time you have taken to complete this survey..</content>
            <!--<link>${NewResponseLink}</link>-->
        </thankYouPage>
    </settings>
    <questions>
        <question id='daytrip' type='YES_NO' time='10' _required='true' text='Are you interested in day trips?' otherField='true'>
            <answer>Yes
                <route>
                    <question id='daytrip_theme' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='What type of day trips interest you?' otherField='true'>
                        <answer>Garden visits</answer>
                        <answer>Winery tours</answer>
                        <answer>Cultural sites</answer>
                        <answer>Bushland parks</answer>
                        <answer>Seaside</answer>
                        <answer>Shopping spree</answer>
                        <answer>Markets</answer>
                        <answer>Festival</answer>
                        <answer>Train ride</answer>
                        <answer>Boat/ferry ride</answer>
                    </question>
                    <question id='daytrip_season' type='MULTIPLE_CHOICE' time='10' _required='true' text='What time of the year would suit you?' otherField='true'>
                        <answer>Summer</answer>
                        <answer>Autumn</answer>
                        <answer>Winter</answer>
                        <answer>Spring</answer>
                    </question>
                    <question id='daytrip_location' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='What destinations would interest you?' otherField='true'>
                        <answer>Yarra Valley</answer>
                        <answer>Mornington Peninsular</answer>
                        <answer>Bellarine Peninsular</answer>
                        <answer>The Dandenongs</answer>
                        <answer>Melbourne City</answer>
                        <answer>Melbourne Suburbs</answer>
                    </question>
                </route>
            </answer>
            <answer>No</answer>
            <!--<answer>Test
                <route>
                    <question id='dance_test' type="TEXTFIELD" text="enter test"/>
                </route>
            </answer>-->
        </question>
        <question id='theatre' type='YES_NO' time='10' _required='true' text='Are you interested in theatre events?' otherField='true'>
            <answer>Yes
                <route>
                    <question id='theatre_level' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='Which level of theatre are you interested in?' otherField='true'>
                        <answer>Amateur</answer>
                        <answer>Professional</answer>
                    </question>
                    <question id='theatre_style' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='What style of theatre are you interested in?' otherField='true'>
                        <answer>Comedy plays</answer>
                        <answer>Comedians</answer>
                        <answer>Light drama</answer>
                        <answer>Musical</answer>
                        <answer>Light Opera</answer>
                        <answer>Opera</answer>
                        <answer>Ballet</answer>
                    </question>
                    <question id='theatre_time' type='MULTIPLE_CHOICE' time='10' _required='true' text='What time of day would suit you?' otherField='true'>
                        <answer>Morning</answer>
                        <answer>Afternoon</answer>
                        <answer>Evening</answer>
                    </question>
                </route>
            </answer>
            <answer>No</answer>
        </question>
        <question id='concert' type='YES_NO' time='10' _required='true' text='Are you interested in concert events?' otherField='true'>
            <answer>Yes
                <route>
                    <question id='concert_level' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='Which level of concert are you interested in?' otherField='true'>
                        <answer>Amateur</answer>
                        <answer>Semi-Professional</answer>
                        <answer>Professional</answer>
                    </question>
                    <question id='concert_style' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='What style of concert are you interested in?' otherField='true'>
                        <answer>Concert band</answer>
                        <answer>Tribute Shows - 50's and 60's</answer>
                        <answer>Tribute Shows - 70's and later</answer>
                        <answer>Classical Music</answer>
                    </question>
                    <question id='concert_time' type='MULTIPLE_CHOICE' time='10' _required='true' text='What time of day would suit you?' otherField='true'>
                        <answer>Morning</answer>
                        <answer>Afternoon</answer>
                        <answer>Evening</answer>
                    </question>
                </route>
            </answer>
            <answer>No</answer>
        </question>
        <question id='dancing' type='YES_NO' time='10' _required='true' text='Are you interested in dancing events?' otherField='true'>
            <answer>Yes
                <route>
                    <question id='dancing_styles' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='Which style of dancing are you interested in?' otherField='true'>
                        <answer>Line dancing</answer>
                        <answer>Ballroom dancing</answer>
                        <answer>Latin</answer>
                        <answer>Square dancing</answer>
                        <answer>Rock and roll</answer>
                    </question>
                    <question id='dancing_time' type='MULTIPLE_CHOICE' time='10' _required='true' text='What time of day would suit you?' otherField='true'>
                        <answer>Morning</answer>
                        <answer>Afternoon</answer>
                        <answer>Evening</answer>
                    </question>
                    <question id='dancing_location' type='MULTIPLE_CHOICE' time='10' _required='true' _sort_options='true' text='What venue would suit you?' otherField='true'>
                        <answer>In the clubhouse</answer>
                        <answer>In a dance studio</answer>
                        <answer>At a social dance venue - eg dinner dance</answer>
                    </question>
                    <question id='dancing_partner' type='YES_NO' time='10' _required='true' text='Do you have a dance partner?' otherField='true'>
                        <answer>Yes</answer>
                        <answer>No</answer>
                    </question>
                </route>
            </answer>
            <answer>No</answer>
            <!--<answer>Test
                <route>
                    <question id='dance_test' type="TEXTFIELD" text="enter test"/>
                </route>
            </answer>-->
        </question>
        <question id="car_pool" type='YES_NO' time='10' _required='true' text='If an event involved car transport would you be able to take other residents?' otherField='true'>
            <answer>Yes
                <route>
                    <question id='car_pool_pax' type='NUMERIC' time='3' _required='true' text='Please enter the number of passengers you can take.' allowDecimals='false' hasMinMax='true' min='1' max='10' numberType='Number'/>
                </route>
            </answer>
            <answer>No
                <route>
                    <question id="car_pool_needed" type='YES_NO' time='10' _required='true' text='Would you need a lift?' otherField='true'/>
                </route>
            </answer>
        </question>
    </questions>
</qrvey>
